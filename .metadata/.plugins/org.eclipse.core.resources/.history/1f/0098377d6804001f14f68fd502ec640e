package javaprogms;

public class swapNos {
	public static void main(String agrs[]) {
		int a=5;
		int b=10;
		b = a - b;
	     System.out.println(b);
		System.out.println(a +","+ b);
		 a = a + b;
	     b = a - b;
	     a = a - b;
	     System.out.println(a +","+ b);
		int c = a;
		a=b;
		b=c;
		System.out.println(a +","+ b);
		
		 a = a + b;
	     b = a - b;
	     a = a - b;
	     System.out.println(a +","+ b);
	}

}
