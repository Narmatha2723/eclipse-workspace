package numArrays;

public class PalindromicSeries {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int n=121,j,rev=0;
	         while(n>0) {
				 j =n%10;
				 rev = rev*10+j;
				n=n/10;
	         }
	         System.out.println(rev);
			
	         if(n==rev) {
					System.out.println("palindrome");
					
				}
			
		

	}

}
